{
	"workbench.colorCustomizations": {
		"statusBar.background": "#1c7549bd",
	},
	"terminal.integrated.scrollback": 1000000000,
	"actionButtons": {
		"reloadButton": "$(refresh)", // vscode icons https://code.visualstudio.com/api/references/icons-in-labels
		"commands": [
			{
				"cwd": "${workspaceFolder}",
				"tooltip":"Open sourced & rviz fixed terminal.",
				"name": "Sourced Terminal",
				"color": "white",
				"singleInstance": true,
				"command": ". ./scripts/sourced_terminal.sh"
			},
			{
				"cwd": "${workspaceFolder}",
				"tooltip": "Format current file",
				"name": "$(request-changes) Format",
				"color": "white",
				"singleInstance": true,
				"command": "./scripts/format.sh ${file}",
			},
			{
				"cwd": "${workspaceFolder}",
				"tooltip": "Build all packages",
				"name": "$(wrench) Build all",
				"color": "white",
				"singleInstance": true,
				"command": "clear && ./scripts/build.sh",
			},
			{
				"cwd": "${workspaceFolder}",
				"tooltip": "Build app",
				"name": "$(wrench) Build custom packages",
				"color": "white",
				"singleInstance": true,
				"command": "clear && ./scripts/build.sh object_detector machine_state_control standalone_trajectory_executor hmi",
			},
			{
				"cwd": "${workspaceFolder}",
				"tooltip": "Test",
				"name": "$(play) Build & Test",
				"color": "white",
				"singleInstance": true,
				"command": "clear && ./scripts/build.sh object_detector machine_state_control standalone_trajectory_executor hmi && ./scripts/test.sh"
			},
			{
				"cwd": "${workspaceFolder}",
				"tooltip": "Test",
				"name": "$(play) Test",
				"color": "white",
				"singleInstance": true,
				"command": "clear && ./scripts/test.sh"
			},
			{
				"cwd": "${workspaceFolder}",
				"tooltip": "Lint",
				"name": "$(play) Lint",
				"color": "white",
				"singleInstance": true,
				"command": "./scripts/lint.sh"
			},
			{
				"cwd": "${workspaceFolder}",
				"tooltip": "Launch",
				"name": "$(play) Launch drivers",
				"color": "white",
				"singleInstance": true,
				"command": "sudo -u $USER -i", // fix environment variables because vscode doesn't load .bashrc
				"extraCommands": [
					"cd ${workspaceFolder}",
					"./scripts/launch_drivers.sh"
				]
			},
			{
				"cwd": "${workspaceFolder}",
				"tooltip": "Launch",
				"name": "$(play) Launch drivers fake",
				"color": "white",
				"singleInstance": true,
				"command": "sudo -u $USER -i", // fix environment variables because vscode doesn't load .bashrc
				"extraCommands": [
					"cd ${workspaceFolder}",
					"./scripts/launch_drivers_fake_robot.sh"
				]
			},
			{
				"cwd": "${workspaceFolder}",
				"tooltip": "Launch",
				"name": "$(play) Launch app",
				"color": "white",
				"singleInstance": true,
				"command": "sudo -u $USER -i", // fix environment variables because vscode doesn't load .bashrc
				"extraCommands": [
					"cd ${workspaceFolder}",
					"./scripts/launch_app.sh"
				]
			},
			{
				"cwd": "${workspaceFolder}",
				"tooltip": "Launch",
				"name": "$(play) Launch app fake",
				"color": "white",
				"singleInstance": true,
				"command": "sudo -u $USER -i", // fix environment variables because vscode doesn't load .bashrc
				"extraCommands": [
					"cd ${workspaceFolder}",
					"./scripts/launch_app_fake_detector.sh"
				]
			}
		]
	},
	"cmakeFormat.args": [
		"--config=${workspaceFolder}/.cmake-format"
	],
